op {
  graph_op_name: "DeepCopy"
  in_arg {
    name: "x"
    description: "The source tensor of type `T`."
  }
  out_arg {
    name: "y"
    description: <<END
    y: A `Tensor` of type `T`. A copy of `x`. Guaranteed that `y`
      is not an alias of `x`.
END
  }
  summary: "Makes a copy of `x`."
}
